<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.reinstall_app">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".ReinstallApplicationClass"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Categories"></activity>
        <activity android:name=".Stats" />
        <activity android:name=".Report" />
        <activity android:name=".AdminRegister" />
        <activity android:name=".AdminLogin" />
        <activity android:name=".MunicipalityRegister" />
        <activity android:name=".MunicipalityLogin" />
        <activity android:name=".ResidentRegister" />
        <activity android:name=".ResidentLogin" />
        <activity android:name=".Login_Register_Page" />
        <activity android:name=".MainActivity" />
        <activity android:name=".RoleSelection">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>